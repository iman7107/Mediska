<body>

    @if (TempData["SuccessMessage"] != null)
    {
        <input type="hidden" id="notify_message_success" value="@TempData["SuccessMessage"].ToString()" />
        TempData["SuccessMessage"] = null;
    }

    @if (TempData["WellcomeMessage"] != null)
    {
        <input type="hidden" id="notify_message_wellcome" value="@TempData["WellcomeMessage"].ToString()" />
        TempData["WellcomeMessage"] = null;
    }

    @if (!string.IsNullOrEmpty(ViewBag.SuccessMessage))
    {
        <div class="alert alert-success z-index msg_div">
            <span class="fa fa-window-close padding_10" onclick="close_msg(this)"></span><strong>عملیات موفق</strong> @ViewBag.SuccessMessage
        </div>
    }


    @if (!string.IsNullOrEmpty(ViewBag.InfoMessage))
    {
        <div class="alert alert-primary z-index msg_div">
            <span class="fa fa-window-close padding_10" onclick="close_msg(this)"></span><strong>پیغام</strong> @ViewBag.InfoMessage
        </div>
    }

    @if (!string.IsNullOrEmpty(ViewBag.ErrorMessage))
    {
        <div class="alert alert-danger z-index msg_div">
            <span class="fa fa-window-close padding_10" onclick="close_msg(this)"></span><strong>خطا</strong> @ViewBag.ErrorMessage
