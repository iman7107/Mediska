<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    @{
        if (string.IsNullOrEmpty(ViewBag.MetaDescription))
        {
            ViewBag.MetaDescription = "خرید آنلاین نرم افزارهای کلینیکی و پزشکی";
        }
        if (string.IsNullOrEmpty(ViewBag.MetaKeywords))
        {
            ViewBag.MetaKeywords = "نرم افزار,فیزیوتراپی,توانبخشی,کاردرمانی,گفتار درمانی,شنوایی سنجی,ارتوپد فنی,طب فیزیکی,زیبایی,پزشکی,کلینیک,بیمه,تامین,سلامت,مسلح,API";
        }
        if (string.IsNullOrEmpty(ViewBag.Title))
        {
            ViewBag.Title = "مدیسکــا";
        }

        int ShopCartCount = 0;
        if (Session["ShopCart"] != null)
        {
            List<int> cart = Session["ShopCart"] as List<int>;
            ShopCartCount = cart.Count;
        }

    }
    <meta name="description" content="@ViewBag.MetaDescription">
    <meta name="keywords" content="@ViewBag.MetaKeywords">
    <meta property="og:site_name" content="مدیســکا" /> <!-- website name -->
    <meta property="og:site" content="https://www.mediska.ir" /> <!-- website link -->
    <meta property="og:title" content="نرم افزارهای پزشکی مدیسکا" /> <!-- title shown in the actual shared post -->
    <meta property="og:description" content="@ViewBag.MetaDescription" /> <!-- description shown in the actual shared post -->
    <meta property="og:image" content="~/Content/image/logo_40.jpg" /> <!-- image link, make sure it's jpg -->
    <meta property="og:url" content="" /> <!-- where do you want your post to link to -->
    <meta property="og:type" content="article" />

    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/png" href="~/Content/image/logo_40.png" />

    <link href="~/Content/Site.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
